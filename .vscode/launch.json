    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'rest_commands'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=rest_commands",
                    "--manifest-path=${workspaceFolder}/core/Cargo.toml"
                ],
                "filter": {
                    "name": "rest_commands",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rest_commands'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rest_commands",
                    "--package=rest_commands",
                    "--manifest-path=${workspaceFolder}/core/Cargo.toml"
                ],
                "filter": {
                    "name": "rest_commands",
                    "kind": "bin"
                }
            },
            "args": [
                "30123",
                "dev/test_commands"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'rest_commands'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=rest_commands",
                    "--package=rest_commands",
                    "--manifest-path=${workspaceFolder}/core/Cargo.toml"
                ],
                "filter": {
                    "name": "rest_commands",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}